{
  "name": "hapi-geolink",
  "version": "0.0.1",
  "description": "Geolink plugin for hapi-server.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "js:clean": "rm -rf public/js/*",
    "js:compile": "webpack -p",
    "build:js": "npm run js:clean && npm run js:compile",
    "css:clean": "rm -rf public/css/*",
    "css:compile": "node-sass src/sass/ -o public/css/ --output-style compressed",
    "css:rename": "renamer \"public/css/*\" --find \".css\" --replace \".min.css\"",
    "css:autoprefix": "postcss -r public/css/* -u autoprefixer",
    "build:css": "npm run css:clean && npm run css:compile && npm run css:rename && npm run css:autoprefix",
    "build": "npm run build:js && npm run build:css"
  },
  "author": "Miguel Serrano",
  "license": "ISC",
  "dependencies": {
    "aguid": "^1.0.4",
    "bcryptjs": "^2.3.0",
    "boom": "^3.1.3",
    "colorbrewer": "^1.0.0",
    "crumb": "^6.0.1",
    "flat": "^2.0.0",
    "foundation-sites": "^6.2.3",
    "jade": "^1.11.0",
    "joi": "^8.1.0",
    "jquery": "2.2.x",
    "jsonwebtoken": "^7.0.0",
    "lodash": "^4.14.2",
    "mssql": "^3.3.0",
    "node-fetch": "^1.5.3",
    "socket.io": "^1.4.6",
    "socketio-jwt": "^4.3.4",
    "topojson": "^1.6.26",
    "twitter-node-client": "0.0.6",
    "whatwg-fetch": "^1.0.0",
    "x-ray": "^2.3.0",
    "yelp": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.7.0",
    "postcss-cli": "^2.5.2",
    "renamer": "^0.5.7",
    "webpack": "^1.13.1"
  }
}
